<?xml version="1.0" encoding="UTF-8"?>

<beans 
xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
      xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
      
  <bean name="f1" class="com.cdac.CoreSpring.factorydi.Company" factory-method="getProduct">
  <constructor-arg value="Cooler"></constructor-arg>
  </bean>
  
  <bean id="p1" class="com.cdac.CoreSpring.factorydi.Product" p:productid="7" p:name="Washing Machine" scope="prototype">
  </bean>
  
  
  </beans>