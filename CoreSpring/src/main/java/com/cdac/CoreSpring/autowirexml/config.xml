<?xml version="1.0" encoding="UTF-8"?>

<beans 
xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
xmlns:context="http://www.springframework.org/schema/context"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
      xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-4.0.xsd">
      
      <context:annotation-config/>
      
      <bean id="instructor1" class="com.cdac.CoreSpring.autowirexml.Instructor" p:name="Mrs.Pretma" p:expertise="Web development"  autowire-candidate="false">
      </bean>
      
      <bean id="inst" class="com.cdac.CoreSpring.autowirexml.Instructor" p:name="Mr.Rajpal Yadav" p:expertise="Android development" >
      </bean>
      
      <bean id="course" class="com.cdac.CoreSpring.autowirexml.Course" p:name="PGDAC" p:duration="7months" autowire="byName">
      </bean>
      
      <bean id="course1" class="com.cdac.CoreSpring.autowirexml.Course" p:name="PGDAC" p:duration="7months" autowire="constructor">
      </bean>
      </beans>